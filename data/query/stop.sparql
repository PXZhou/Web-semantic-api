PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ex:    <http://example.com/stations/>

SELECT ?label
WHERE {
  ?ex rdfs:label ?label;
} GROUP BY ?label 


PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ex: <http://example.com/stations/>
PREFIX geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>

SELECT ?lat ?long
WHERE {
  ?ex rdfs:label ?label.
  ?ex geo:lat ?lat.
  ?ex geo:long ?long
  FILTER (?label ="Gare de Margaux"@fr)
} GROUP BY ?label
